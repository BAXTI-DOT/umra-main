{
  "name": "template-http",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "./node_modules/.bin/nest build",
    "start": "./node_modules/.bin/nest start",
    "start:dev": "./node_modules/.bin/nest start --watch",
    "start:debug": "./node_modules/.bin/nest start --watch --debug",
    "start:prod": "node ./dist/main.js",
    "test": "./node_modules/.bin/jest",
    "test:watch": "./node_modules/.bin/jest --watch",
    "test:debug": "node --inspect-brk -r ts-node/register -r tsconfig-paths/register ./node_modules/.bin/jest --runInBand",
    "test:coverage": "./node_modules/.bin/jest --coverage",
    "test:int": "./node_modules/.bin/jest --config ./jest-int.json",
    "test:int:watch": "./node_modules/.bin/jest --config ./jest-int.json --watch",
    "test:int:debug": "node --inspect-brk -r ts-node/register -r tsconfig-paths/register ./node_modules/.bin/jest --config ./jest-int.json --runInBand",
    "test:e2e": "./node_modules/.bin/jest --config ./jest-e2e.json",
    "test:e2e:watch": "./node_modules/.bin/jest --config ./jest-e2e.json --watch",
    "test:e2e:debug": "node --inspect-brk -r ts-node/register -r tsconfig-paths/register ./node_modules/.bin/jest --config ./jest-e2e.json --runInBand",
    "docs:build": "./node_modules/.bin/compodoc --output ./docs --tsconfig ./tsconfig.json",
    "docs:serve": "./node_modules/.bin/compodoc --output ./docs --tsconfig ./tsconfig.json --serve",
    "prisma:format": "./node_modules/.bin/prisma format",
    "prisma:validate": "./node_modules/.bin/prisma validate",
    "prisma:generate": "./node_modules/.bin/prisma generate",
    "migrate:create": "./node_modules/.bin/prisma migrate dev",
    "migrate:deploy": "./node_modules/.bin/prisma migrate deploy",
    "migrate:resolve": "./node_modules/.bin/prisma migrate resolve",
    "migrate:status": "./node_modules/.bin/prisma migrate status",
    "migrate:reset": "./node_modules/.bin/prisma migrate reset"
  },
  "dependencies": {
    "@nestjs/common": "^9.2.0",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.2.0",
    "@nestjs/microservices": "^9.2.1",
    "@nestjs/platform-express": "^9.2.0",
    "@nestjs/swagger": "^6.1.3",
    "@prisma/client": "^4.10.1",
    "@types/uuid": "^9.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "jsonwebtoken": "^9.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.5.7",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@compodoc/compodoc": "^1.1.19",
    "@nestjs/cli": "^9.1.5",
    "@nestjs/schematics": "^9.0.3",
    "@nestjs/testing": "^9.2.0",
    "@types/express": "^4.17.14",
    "@types/jest": "^29.2.3",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/node": "^18.11.9",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.44.0",
    "@typescript-eslint/parser": "^5.44.0",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.3.1",
    "prettier": "^2.7.1",
    "prisma": "^4.6.1",
    "supertest": "^6.3.1",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^4.9.3"
  },
  "jest": {
    "rootDir": "./src",
    "verbose": true,
    "testRegex": ".*\\.*spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "testEnvironment": "node",
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/../src/$1"
    },
    "moduleFileExtensions": [
      "ts",
      "js",
      "json"
    ],
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage"
  }
}
